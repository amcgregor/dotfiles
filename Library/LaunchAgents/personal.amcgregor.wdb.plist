<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>personal.amcgregor.wdb</string>
    <key>UserName</key>
    <string>amcgregor</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/wdb.server.py</string>
      <string>--show-filename</string>
      <string>--theme=material</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/wdb.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/wdb.log</string>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
  </dict>
</plist>
