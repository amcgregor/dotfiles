<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>homebrew.mxcl.znc</string>
	<key>KeepAlive</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/opt/znc/bin/znc</string>
		<string>--foreground</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>UserName</key>
	<string>amcgregor</string>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/znc.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/znc.log</string>
	<key>StartInterval</key>
	<integer>300</integer>
</dict>
</plist>
